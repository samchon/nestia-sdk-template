{
  "name": "@ORGANIZATION/PROJECT-api",
  "version": "0.1.0",
  "description": "SDK library generated by Nestia",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib && tsc",
    "build:test": "rimraf bin && tsc --project test/tsconfig.json",
    "deploy": "npm run build && npm publish",
    "dev": "npm run build:test -- --watch",
    "prepare": "ts-patch install && typia patch",
    "start": "npx ts-node test/start.ts",
    "test": "npx ts-node test/index.ts",
    "test:simulate": "npx ts-node test/index.ts --simulate true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/nestia"
  },
  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/nestia/issues"
  },
  "homepage": "https://nestia.io",
  "files": [
    "lib",
    "swagger.json",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@nestia/e2e": "^0.4.1",
    "@types/inquirer": "8.2.5",
    "commander": "^10.0.0",
    "inquirer": "8.2.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "typescript": "^5.3.3",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@nestia/fetcher": "^2.5.15",
    "typia": "^5.5.4"
  }
}